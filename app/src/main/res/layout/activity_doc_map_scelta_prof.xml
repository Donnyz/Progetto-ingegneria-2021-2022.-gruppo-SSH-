<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/doc_map_scelta"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:onClick="goto_mappa"
    android:background="#6969EC"
    tools:context=".doc_map_scelta">

    <include
        android:id="@+id/toolbar_doc_mappa_sceltat"
        layout="@layout/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="parent" />

    <Button
        android:id="@+id/Bdocumento"
        android:layout_width="249dp"
        android:layout_height="48dp"
        android:background="#ffffff"
        android:onClick="goto_documento"
        android:text="@string/documento"
        android:textColor="#6969EC"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.456"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.374"
        tools:ignore="UsingOnClickInXml" />

    <Button
        android:id="@+id/BMappa"
        android:layout_width="249dp"
        android:layout_height="48dp"
        android:background="#ffffff"
        android:textColor="#6969EC"
        android:textStyle="bold"
        android:textSize="18dp"
        android:onClick="goto_mappa"
        android:text="@string/mappa"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.456"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.554"
        tools:ignore="UsingOnClickInXml" />

</androidx.constraintlayout.widget.ConstraintLayout>